nd;\n    padding-right: 10px;\n    margin-top: 10px;\n    width: 80%;\n    height:35px;\n    border-radius: 40px;\n    border: 1.5px #264653 solid;\n    background-color: white;\n}\n\n.search-icon{\n    max-width:20px; \n    max-height:20px;\n}\n\n.genre-search-bar,.title-search-bar,.author-search-bar{\n    width: 80%;\n    border: 0px transparent;\n}\n\n.search-buttons-container{\n    display: flex;\n    justify-content: center;\n    width: 100%;\n    padding-top: 20px;\n}\n\n.search-button{\n    width: 35%;\n    height: 30px;\n    background-color: #2A9D8F;\n    color: white;\n    border-radius: 20px;\n    border: 1.5px #264653 solid;\n    margin-right: 5px;\n    cursor: pointer;\n}\n\n.advanced-search-button {\n    width: 40%;\n    height: 30px;\n    background-color: #264653;\n    color: white;\n    border-radius: 20px;\n    border: 1.5px #264653 solid;\n    margin-left: 5px;\n    cursor: pointer;\n}\n\n.advanced-search-input-container{\n    padding-top: 5px;\n    width: 100%;\n    display: none;\n}\n\n.advanced-search-input-container.active{\n    display: flex;\n    align-items: center;\n    justify-content: space-around;\n    flex-direction: column;\n}\n\n/* Mobile */\n@media only screen and (max-width: 450px) {\n\n}\n\n/* Tablet Styles */\n@media only screen and (min-width: 451px) and (max-width: 960px) {\n    \n    .search-bars-container{\n        width: 70%;\n        height: 40%;\n    }\n\n    .icon-info{\n        padding-right: 55px;\n    }\n\n    .search-button{\n        width: 25%;\n        height: 30px;\n        margin-right: 10px;\n    }\n    \n    .advanced-search-button {\n        width: 30%;\n        height: 30px;\n        margin-left: 10px;\n    }\n\n}\n\n/* Desktop Styles */\n@media only screen and (min-width: 961px) {\n\n    .search-bars-container{\n        width: 50%;\n        height: 40%;\n    }\n\n    .icon-info{\n        padding-right: 55px;\n    }\n\n    .search-button{\n        width: 25%;\n        height: 30px;\n        margin-right: 10px;\n    }\n    \n    .advanced-search-button {\n        width: 30%;\n        height: 30px;\n        margin-left: 10px;\n    }\n\n}\n\n@media only screen and (min-width: 1440px) {\n    \n    .search-bars-container{\n        width: 40%;\n        height: 40%;\n    }\n\n    .icon-info{\n        padding-right: 65px;\n    }\n\n    .search-button{\n        width: 25%;\n        height: 30px;\n        margin-right: 10px;\n    }\n    \n    .advanced-search-button {\n        width: 30%;\n        height: 30px;\n        margin-left: 10px;\n    }\n\n}\n\n/* 4K */\n@media only screen and (min-width: 2560px) {\n\n    .search-bars-container{\n        width: 20%;\n        height: 40%;\n    }\n\n    .icon-info{\n        padding-right: 65px;\n    }\n\n    .search-button{\n        width: 25%;\n        height: 30px;\n        margin-right: 10px;\n    }\n    \n    .advanced-search-button {\n        width: 30%;\n        height: 30px;\n        margin-left: 10px;\n    }\n\n}\n\n/* Keyframes */\n@keyframes wiggle {\n    0%, 7% {\n      transform: rotateZ(0);\n    }\n    15% {\n      transform: rotateZ(-15deg);\n    }\n    20% {\n      transform: rotateZ(10deg);\n    }\n    25% {\n      transform: rotateZ(-10deg);\n    }\n    30% {\n      transform: rotateZ(6deg);\n    }\n    35% {\n      transform: rotateZ(-4deg);\n    }\n    40%, 100% {\n      transform: rotateZ(0);\n    }\n  }\n\n"},"lineCount":null}},"error":null,"hash":"1f827540355df77e30449d6ff6ac7f94","cacheData":{"env":{}}}